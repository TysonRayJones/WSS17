          TagBox[
           RowBox[{"(", 
            RowBox[{"2", ",", "0"}], ")"}],
           Derivative],
          MultilineFunction->None], "[", 
         RowBox[{"x", ",", "t"}], "]"}]}], " ", "+", " ", 
       RowBox[{"potential", " ", 
        RowBox[{"\[Psi]", "[", 
         RowBox[{"x", ",", "t"}], "]"}]}]}]}], ",", "\n", "\t\t", 
     RowBox[{
      RowBox[{"\[Psi]", "[", 
       RowBox[{"x", ",", " ", "0"}], "]"}], " ", "\[Equal]", " ", 
      RowBox[{
       RowBox[{"UnitStep", "[", 
        RowBox[{"x", " ", "-", " ", 
         RowBox[{"(", 
          RowBox[{"xL", "+", "0.01"}], ")"}]}], "]"}], " ", "-", " ", 
       RowBox[{"UnitStep", "[", 
        RowBox[{"x", " ", "-", " ", 
         RowBox[{"(", 
          RowBox[{"xR", "-", "0.01"}], ")"}]}], "]"}]}]}], ",", "\n", "\t\t", 
     RowBox[{
      RowBox[{"\[Psi]", "[", 
       RowBox[{"xL", ",", "t"}], "]"}], " ", "\[Equal]", " ", "0"}], ",", 
     "\n", "\t\t", 
     RowBox[{
      RowBox[{"\[Psi]", "[", 
       RowBox[{"xR", ",", "t"}], "]"}], " ", "\[Equal]", " ", "0"}]}], "\n", 
    "\t", "}"}]}], ",", "\t", "\n", "\n", 
  RowBox[{
   RowBox[{"getGroundstate", "[", 
    RowBox[{"potential_", ",", " ", 
     RowBox[{"domain_:", 
      RowBox[{"{", 
       RowBox[{"x", ",", 
        RowBox[{"-", "5"}], ",", "5"}], "}"}]}], ",", " ", 
     RowBox[{"timestep_:", "0.1"}], ",", " ", 
     RowBox[{"threshold_:", "0.01"}]}], "]"}], " ", ":=", "\n", "\t", 
   RowBox[{"Module", "[", "\n", "\t\t", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "x", ",", " ", "t", ",", " ", "psi", ",", " ", "\[Psi]term", ",", " ", 
       "state", ",", " ", "init\[Psi]", ",", " ", "prevE", ",", " ", "curE"}],
       "}"}], ",", "\n", "\t\t", 
     RowBox[{
      RowBox[{"x", " ", "=", " ", 
       RowBox[{"domain", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", "\n", "\t\t", 
      RowBox[{"psi", " ", "=", " ", 
       RowBox[{"initialWavefunction", "[", "domain", "]"}]}], ";", "\n", 
      "\t\t", 
      RowBox[{"prevE", " ", "=", " ", 
       RowBox[{"expectedEnergy", "[", 
        RowBox[{"init\[Psi]", ",", " ", "potential", ",", " ", "domain"}], 
        "]"}]}], ";", "\n", "\t\t", 
      RowBox[{"curE", " ", "=", " ", 
       RowBox[{
        RowBox[{"Abs", "[", "prevE", "]"}], " ", "+", " ", 
        RowBox[{"1000", " ", "threshold"}]}]}], ";", "\n", "\t\t", "\n", 
      "\t\t", 
      RowBox[{"state", " ", "=", " ", 
       RowBox[{"NDSolve`ProcessEquations", "[", "\n", "\t\t\t", 
        RowBox[{
         RowBox[{"getEquations", "[", 
          RowBox[{
          "potential", ",", " ", "domain", ",", " ", "\[Psi]term", ",", " ", 
           "t"}], "]"}], ",", "\n", "\t\t\t", "\[Psi]term", ",", " ", 
         "domain", ",", " ", "t", ",", " ", 
         RowBox[{
         "Method", " ", "\[Rule]", " ", "\"\<ExplicitRungeKutta\>\""}]}], 
        "]"}]}], ";", "\n", "\t\t", "\n", "\t\t", 
      RowBox[{"While", "[", "\n", "\t\t\t", 
       RowBox[{
        RowBox[{
         RowBox[{"Abs", "[", 
          FractionBox[
           RowBox[{"curE", " ", "-", " ", "prevE"}], 
           RowBox[{"curE", " ", "timestep"}]], "]"}], " ", ">", " ", 
         "threshold"}], ",", "\n", "\t\t\t", 
        RowBox[{
         RowBox[{"prevE", " ", "=", " ", "curE"}], ";", "\n", "\t\t\t", "\n", 
         "\t\t\t", 
         RowBox[{
          RowBox[{
           RowBox[{"NDSolve`Iterate", "[", 
            RowBox[{"state", ",", " ", "timestep"}], "]"}], "\n", "\t\t\t", 
           "psi"}], " ", "=", " ", 
          RowBox[{
           RowBox[{"NDSolve`ProcessSolutions", "[", 
            RowBox[{"state", ",", " ", "\"\<Forward\>\""}], "]"}], "[", 
           RowBox[{"[", 
            RowBox[{"1", ",", " ", "2"}], "]"}], "]"}], "\n", "\t\t\t", "\n", 
          "\t\t\t", "\n", "\t\t\t", 
          RowBox[{"(*", " ", 
