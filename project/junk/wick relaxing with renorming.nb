Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"getEquations", "[", 
    RowBox[{"potential_", ",", " ", 
     RowBox[{"{", 
      RowBox[{"x_", ",", " ", "xL_", ",", " ", "xR_"}], "}"}], ",", " ", 
     RowBox[{"\[Psi]_:", "\[Psi]"}], ",", " ", 
     RowBox[{"t_:", "t"}]}], "]"}], " ", ":=", "\n", "\t", 
   RowBox[{"{", "\n", "\t\t", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       RowBox[{
        SuperscriptBox["\[Psi]", 
         TagBox[
          RowBox[{"(", 
           RowBox[{"0", ",", "1"}], ")"}],
          Derivative],
         MultilineFunction->None], "[", 
        RowBox[{"x", ",", "t"}], "]"}]}], " ", "\[Equal]", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"-", 
         FractionBox["1", "2"]}], " ", 
        RowBox[{
